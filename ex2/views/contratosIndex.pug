block content
    head
        title=contrato._id
        meta(charset='utf-8')
        link(rel='icon' href='favicon.png')
        link(rel='stylesheet' href='/w3.css')
    body
        .w3-card-4
            header.w3-container.w3-green
                h1=contrato._id
            .w3-container
                ul.w3-ul.w3-card-4(style='width:50%')
                    li
                        b Número de Anúncio:
                        |  #{contrato.nAnuncio}
                    li
                        b Tipo de contrato:
                        | #{contrato.tipo}
                    li
                        b Objeto do contrato:
                        | #{contrato.objetoContrato}
                    li
                        b Data publicação:
                        | #{contrato.dataPublicacao}
                    li
                        b Data de celebração do contrato:
                        | #{contrato.dataCelebracaoContrato}
                    li
                        b Preço contratual:
                        | #{contrato.precoContratual}
                    li
                        b Prazo de execução:
                        | #{contrato.prazoExecucao}
                    li
                        b NIPC da entidade comunicante:
                        | #{contrato.NIPC_entidade}
                    li
                        b Entidade comunicante:
                        | #{contrato.entidade}
                    li
                        b Fundamentação:
                        | #{contrato.fundamentacao}
            footer.w3-container.w3-green
                address
                | Gerado por galuno::EngWeb2024 - [
                a(href='/') Voltar
                | ]
